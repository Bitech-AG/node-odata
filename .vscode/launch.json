  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha single Test",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": ["--preserve-symlinks"],
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "--require",
        "@babel/register",
        "--reporter",
        "dot",
        "--timeout",
        "300000",
        "test/odata.batch.js"
      ]
    },
    {
        "type": "node",
        "request": "launch",
        "name": "Launch Complex Resource",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "program": "${workspaceFolder}/examples/complex-resource/index.js"
    }
  ]
}